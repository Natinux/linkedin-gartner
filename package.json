    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-util": "latest",
    "gulp-watch": "^4.3.10",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mkdirp": "latest",
    "mocha": "^3.2.0",
    "mock": "^0.1.1",
    "nodent-loader": "^1.2.6",
    "redux-logger": "^2.7.4",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-stream": "^3.2.0"
  }
}
