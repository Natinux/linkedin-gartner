{
  "name": "linkedin-gartner",
  "version": "1.0.0",
  "description": "Linkedin Public Profile Parser Test Application",
  "main": "index.js",
  "scripts": {
    "build": "node node_modules/gulp/bin/gulp.js",
    "build-docker": "docker build -t gertner/linkedin-gartner -f ./assets/docker/dockerfile.yml .",
    "start": "cd ./dist/server && node app.js",
    "start-docker": "docker-compose -f ./assets/docker/compose/run.yml up",
    "stop-docker": "docker-compose -f ./assets/docker/compose/run.yml down",
    "test": "node dist/server/test/mocha-runner.js dist/server/test/**/*.spec.js",
    "test-docker": "docker-compose -f ./assets/docker/compose/test.yml run linkedin-gartner",
    "profile-fetcher": "node dist/server/workers/profileParser.js",
    "config-then-test": "gulp config --env test && npm test",
    "config-then-run": "gulp config && npm start",
    "config-then-run-worker": "gulp config && npm run profile-fetcher"
  },
  "author": "Nati Nazarov",
  "license": "ISC",
  "babel": {
    "plugins": [
      "add-module-exports",
      "transform-react-jsx",
      "syntax-async-functions",
      [
        "fast-async",
        {
          "env": {
            "augmentObject": false,
            "dontMapStackTraces": true,
            "dontInstallRequireHook": true
          },
          "compiler": {
            "promises": true,
            "generators": false
          },
          "runtimePattern": null,
          "useRuntimeModule": false
        }
      ]
    ],
    "presets": [
      "stage-0",
      "node6"
    ]
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "bowser": "^1.5.0",
    "bunyan": "^1.8.4",
    "bunyan-prettystream": "^0.1.3",
    "bunyan-request": "^1.2.0",
    "cheerio": "^0.22.0",
    "cors": "^2.8.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "express-useragent": "^1.0.4",
    "express-validator": "^2.21.0",
    "font-awesome": "^4.7.0",
    "form-urlencoded": "^1.5.0",
    "glob": "latest",
    "graceful-fs": "^4.1.10",
    "live-server": "^1.1.0",
    "moment": "^2.16.0",
    "mongoose": "^4.6.8",
    "mongoose-paginate": "^5.0.3",
    "nconf": "^0.8.4",
    "node-fetch": "^1.6.3",
    "query-string": "^4.2.3",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-spinkit": "^1.1.11",
    "react-tag-input": "^4.4.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "retry": "^0.10.0",
    "selectn": "^1.1.1",
    "through2": "^2.0.1",
    "watchify": "^3.7.0",
    "yargs": "^6.4.0"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.18.0",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-es2015-computed-properties": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.18.0",
    "babel-plugin-transform-es2015-duplicate-keys": "^6.8.0",
    "babel-plugin-transform-es2015-for-of": "^6.18.0",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-literals": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-es2015-object-super": "^6.8.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.18.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.8.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.18.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.11.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.0",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fast-async": "^6.1.2",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-apidoc": "^0.2.4",
    "gulp-babel": "^6.1.2",
